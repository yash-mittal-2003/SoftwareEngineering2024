                                    <DoubleAnimation Storyboard.TargetProperty="(Ellipse.RenderTransform).(ScaleTransform.ScaleX)"
                                                     From="1" To="1.5" Duration="0:0:0.5" AutoReverse="True" BeginTime="0:0:0"/>
                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </Ellipse.Triggers>
                </Ellipse>
                <!-- Repeat for additional dots -->
            </StackPanel>
        </StackPanel>
