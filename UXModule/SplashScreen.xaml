<Window x:Class="UXModule.SplashScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Icon= "\Images\edulinkone-websiteicon.ico"
        Title="MainWindow"
        WindowState="Maximized">
    <Grid>

        <!-- Animated Gradient Background -->
        <Rectangle>
            <Rectangle.Fill>
                <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                    <GradientStop Color="#190019" Offset="0.0" x:Name="GradientStop1"/>
                    <GradientStop Color="#2B124C" Offset="1.0" x:Name="GradientStop2"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
            <Rectangle.Triggers>
                <EventTrigger RoutedEvent="Rectangle.Loaded">
                    <BeginStoryboard >
                        <Storyboard x:Name="BackgroundStoryboard" RepeatBehavior="Forever">
                            <ColorAnimation Storyboard.TargetName="GradientStop1" Storyboard.TargetProperty="Color"
                                            From="#190019" To="#2B124C" Duration="0:0:3" AutoReverse="True"/>
                            <ColorAnimation Storyboard.TargetName="GradientStop2" Storyboard.TargetProperty="Color"
                                            From="#190019" To="#2B124C" Duration="0:0:3" AutoReverse="True"/>
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Rectangle.Triggers>
        </Rectangle>

